{
  "name": "simform-expo-cli",
  "version": "1.0.0",
  "description": "A interactive cli that generates the boilerplate as per your projects requirement",
  "type": "module",
  "main": "lib/cli.js",
  "bin": "lib/cli.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "npx simform-expo-cli",
    "build": "rm -rf lib && tsc && tsc-alias",
    "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts --fix",
    "format": "prettier --write --parser typescript '**/*.ts'",
    "release": "semantic-release"
  },
  "keywords": [
    "react-native",
    "expo",
    "boilerplate",
    "cli"
  ],
  "author": "Simform Solutions Pvt Ltd",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SimformSolutionsPvtLtd/simform-expo-cli.git"
  },
  "bugs": {
    "url": "https://github.com/SimformSolutionsPvtLtd/simform-expo-cli/issues"
  },
  "homepage": "https://github.com/SimformSolutionsPvtLtd/simform-expo-cli",
  "dependencies": {
    "chalk": "^5.3.0",
    "fs": "^0.0.1-security",
    "inquirer": "^12.1.0",
    "lodash": "^4.17.21",
    "ora": "^8.1.1",
    "path": "^0.12.7",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/lodash": "^4",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "concurrently": "^9.1.0",
    "cspell": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "lefthook": "^1.8.4",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "semantic-release": "^24.2.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@4.5.1"
}
